{
	"presets": [
		[
			"@babel/preset-env",
			{
				"modules": false,
				"loose": true,
				"targets": "node 16",
				"useBuiltIns": "usage",
				"corejs": {
					"version": "3.37",
					"proposals": true
				}
			}
		]
	],
	"plugins": [
		["@babel/plugin-proposal-decorators", { "legacy": true }],
		["@babel/plugin-proposal-class-properties", { "legacy": true }]
	]
}
